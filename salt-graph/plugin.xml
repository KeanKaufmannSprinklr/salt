<plugin id="de.hu_berlin.german.korpling.saltnpepper.salt.salt-graph" name="salt-graph" version="1.1.3-SNAPSHOT">

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="graph"
            class="de.hu_berlin.german.korpling.saltnpepper.salt.graph.GraphPackage"
            genModel="model/graph.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated graph -->
      <package
            uri="index"
            class="de.hu_berlin.german.korpling.saltnpepper.salt.graph.index.IndexPackage"
            genModel="src/main/resources/model/graph.genmodel"/>
   </extension>

</plugin>
